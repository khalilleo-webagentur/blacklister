{% extends 'dashboard/layout/base.html.twig' %}

{% block title %} Dashboard Documentation overview {% endblock %}

{% block body %}

<div class="content-wrapper">


    <div class="content-header">
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-6">
                    <h5 class="m-0 text-dark mt-1">Documentation overview</h5>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right"></ol>
                </div>
            </div>
        </div>
    </div>

    <section class="content">
        <div class="container-fluid">

            <div class="row">
                <div class="col-lg-12">
                    <h5>Domain</h5>
                    <ul class="fs-14">
                        <li>Method: GET</li>
                        <li>Authorization type: Bearer Token Example: eadd5e27-c6b0-4e8c-8272-54554767cc0e</li>
                        <li>Headers > User-Agent: Awesome-App</li>
                    </ul>
                    <p>Example: http://localhost:8080/api/v1/domain?domain=example.com</p>
                    <pre class="fs-13 text-secondary py-0">
{
  "success": true,
  "message": "on blacklist"
}
                    </pre>
                    <hr>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <h5>Username</h5>
                    <ul class="fs-14">
                        <li>Method: GET</li>
                        <li>Authorization type: Bearer Token Example: eadd5e27-c6b0-4e8c-8272-54554767cc0e</li>
                        <li>Headers > User-Agent: Awesome-App</li>
                    </ul>
                    <p>Example: http://localhost:8080/api/v1/username?username=johne_doe</p>
                    <pre class="fs-13 text-secondary py-0">
{
  "success": true,
  "message": "on blacklist"
}
                    </pre>
                    <hr>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <h5>E-mail</h5>
                    <ul class="fs-14">
                        <li>Method: GET</li>
                        <li>Authorization type: Bearer Token Example: eadd5e27-c6b0-4e8c-8272-54554767cc0e</li>
                        <li>Headers > User-Agent: Awesome-App</li>
                    </ul>
                    <p>Example: http://localhost:8080/api/v1/email?email=j.doe@example.com</p>
                    <pre class="fs-13 text-secondary py-0">
{
  "success": true,
  "message": "on blacklist"
}
                    </pre>
                    <hr>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-12">
                    <h5>IP-Address</h5>
                    <ul class="fs-14">
