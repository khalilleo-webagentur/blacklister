<div class="card-body table-responsive bg-white p-0 mb-3">
    <table class="table table-hover text-nowrap">
        <thead>
            <tr class="fs-14">
                <th>#</th>
                <th>{{ 'User'|trans }}</th>
                <th>{{ 'Username'|trans }}</th>
                <th>{{ 'Email'|trans }}</th>
                <th>{{ 'Domain'|trans }}</th>
                <th>{{ 'IP'|trans }}</th>
                <th>{{ 'URL'|trans }}</th>
                <th>{{ 'Updated'|trans }}</th>
                <th>{{ 'Created'|trans }}</th>
                <th>{{ 'Action'|trans }}</th>
            </tr>
        </thead>
        <tbody>
            {% for row in blacklists %}
            <tr class="fs-14">
                <td>{{ row.id }}</td>
                <td>
                    <a href="{{path('app_dashboard_user_edit', {id:row.user.id})}}">
                        {{ row.user.name }}
                    </a>
                </td>
                <td>{{ row.username ? row.username|capitalize : '---' }}</td>
                <td>{{ row.email ? row.email : '---' }}</td>
                <td>{{ row.domain ? row.domain : '---' }}</td>
                <td>{{ row.ipAddress ? row.ipAddress : '---' }}</td>
                <td>{{ row.url ? row.url : '---' }}</td>

                <td title="{{ timeAgo(row.updatedAt) }}">{{ row.updatedAt|date }}</td>
                <td title="{{ timeAgo(row.createdAt) }}">{{ row.createdAt|date }}</td>

                <td>
                    <a href="#" class="text-danger">
                        <span class="fa fa-edit fa-sm"></span>
                    </a>
                </td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>